{
  "name": "educore",
  "version": "1.0.0",
  "description": "Educational Management System",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run supabase:start\" \"npm run client:dev\"",
    "start": "node server.js",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase db reset",
    "supabase:generate-types": "npx supabase gen types typescript --local > types/supabase.ts",
    "db:migrate": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "functions:deploy": "npx supabase functions deploy",
    "functions:serve": "npx supabase functions serve"
  },
  "keywords": [
    "education",
    "management",
    "supabase"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "@date-io/date-fns": "^3.2.1",
    "@mui/x-date-pickers": "^8.7.0",
    "@sentry/node": "^7.88.0",
    "@sentry/react": "^7.88.0",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/react-datepicker": "^6.2.0",
    "@uiw/react-markdown-preview": "^5.1.4",
    "@uiw/react-md-editor": "^4.0.7",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.3",
    "helmet": "^7.1.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "react": "^18.2.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^11.0.5",
    "resend": "^4.6.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "supabase": "^2.30.4",
    "typescript": "^5.3.3"
  }
}
